{
  "name": "@meese-os/client",
  "version": "2.5.8",
  "description": "meeseOS client",
  "main": "dist/main.js",
  "style": "dist/main.css",
  "typings": "index.d.ts",
  "files": [
    "index.d.ts",
    "dist/main.js",
    "dist/main.js.map",
    "dist/main.css",
    "dist/main.css.map",
    "dist/*.png",
    "README.md"
  ],
  "scripts": {
    "test": "npm run eslint && npm run stylelint && npm run jest",
    "jest": "jest",
    "coverage": "jest --coverage",
    "eslint": "eslint index.js 'src/**/*.js'",
    "build": "webpack",
    "watch": "webpack --watch",
    "stylelint": "stylelint index.scss src/**/*.scss  --fix",
    "prepublishOnly": "npm run test && rm ./dist/* && NODE_ENV=production npm run build"
  },
  "devDependencies": {
    "@meese-os/eslint-config": "workspace:*",
    "@meese-os/stylelint-config": "workspace:*",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.1",
    "eslint": "^8.25.0",
    "eslint-plugin-jest": "^26.5.3",
    "html-loader": "^3.1.0",
    "html-loader-jest": "^0.2.1",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "jest": "^26.0.0"
  },
  "dependencies": {
    "@meese-os/common": "workspace:*",
    "@meese-os/cursor-effects": "workspace:*",
    "@meese-os/dynamic-wallpapers": "workspace:*",
    "@meese-os/event-emitter": "workspace:*",
    "@meese-os/gui": "workspace:*",
    "@meese-os/sounds": "workspace:*",
    "css-mediaquery": "^0.1.2",
    "deepmerge": "^4.2.2",
    "hyperapp": "^1.2.10",
    "js-cookie": "^3.0.1",
    "simplejsonconf": "^2.0.7",
    "is-plain-object": "^5.0.0",
    "express-session": "^1.17.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "plugins": ["jest"],
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": ["@meese-os/eslint-config", "plugin:jest/recommended"]
  },
  "stylelint": {
    "extends": ["stylelint-config-standard-scss", "@meese-os/stylelint-config"]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "esdoc": {
    "source": "./src",
    "destination": "./doc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "brand": {
            "title": "meeseOS Core API",
            "description": "meeseOS Core API Documentation"
          },
          "lint": {
            "enable": false
          },
          "coverage": {
            "enable": false
          },
          "undocumentIdentifier": {
            "enable": false
          }
        }
      },
      {
        "name": "esdoc-publish-html-plugin"
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "all": true
        }
      }
    ]
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "jsdom",
    "coverageReporters": [
      "lcov"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/setup.js"
    ],
    "moduleNameMapper": {
      "^meeseOS$": "<rootDir>/__mocks__/core.js",
      "^.+\\.html?$": "html-loader-jest",
      "^.+\\.scss$": "<rootDir>/__mocks__/null-module.js",
      "^.+\\.(png|jpe?g|gif)$": "<rootDir>/__mocks__/null-module.js"
    },
    "coveragePathIgnorePatterns": [
      "src/config.js",
      "src/providers",
      "/node_modules/"
    ]
  }
}
