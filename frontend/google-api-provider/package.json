{
  "name": "@meese-os/google-api-provider",
  "version": "1.0.1",
  "description": "Google API Service Provider for meeseOS",
  "scripts": {
    "test": "npm run eslint",
    "eslint": "eslint *.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rd /s /q dist>nul 2>&1|echo.>nul && npm run build"
  },
	"license": "BSD-2-Clause",
  "files": [
    "dist/",
    "README.md"
  ],
  "devDependencies": {
    "@meese-os/eslint-config": "workspace:*",
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.11",
    "babel-loader": "^9.1.3",
    "eslint": "^8.48.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
	"dependencies": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 11
    },
    "extends": "@meese-os/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
