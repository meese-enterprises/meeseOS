{
  "name": "@meese-os/old-site",
  "version": "1.0.6",
  "description": "Old aaronmeese.com design",
  "scripts": {
    "eslint": "eslint *.js",
    "test": "npm run eslint",
    "build": "webpack",
    "watch": "webpack --watch",
    "stylelint": "stylelint index.scss src/**/*.scss  --fix",
    "prepublishOnly": "npm run eslint && npm run stylelint && rm ./dist/* && npm run build"
  },
  "files": [
    "dist/",
    "server.js",
    "metadata.json"
  ],
  "devDependencies": {
    "@meese-os/eslint-config": "workspace:*",
    "@meese-os/stylelint-config": "workspace:*",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.1",
    "eslint": "^8.25.0",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@babel/preset-react": "^7.18.6",
    "postcss": "^8.4.17",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "style-loader": "^3.3.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@react-hook/window-size": "3.1.1",
    "axios": "^1.1.2",
    "bootstrap": "^5.2.2",
    "dotenv": "^16.0.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "react-typical": "^0.1.3"
  },
  "meeseOS": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@meese-os/eslint-config"
  },
  "stylelint": {
    "extends": ["stylelint-config-standard-scss", "@meese-os/stylelint-config"]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
