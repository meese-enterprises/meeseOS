{
  "name": "@meese-os/image-to-8bit",
  "version": "1.0.2",
  "description": "MeeseOS Image to 8bit application",
  "scripts": {
    "eslint": "eslint *.js",
    "test": "npm run eslint",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rm -rf ./dist/* && npm run build"
  },
  "files": [
    "dist/",
    "metadata.json"
  ],
  "devDependencies": {
    "@meese-os/eslint-config": "workspace:*",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.25.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {},
  "meeseOS": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@meese-os/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
