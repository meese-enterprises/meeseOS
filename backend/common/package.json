{
  "name": "@meese-os/common",
  "version": "1.0.1",
  "description": "meeseOS Common Modules",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "typings": "index.d.ts",
  "files": [
    "index.d.ts",
    "dist/main.js.map",
    "dist/main.js",
    "README.md"
  ],
  "scripts": {
    "eslint": "eslint index.js 'src/**/*.js'",
    "test": "npm run eslint",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rm ./dist/* && NODE_ENV=production npm run build"
  },
  "devDependencies": {
    "@meese-os/eslint-config": "workspace:*",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.25.0",
    "stylelint": "^14.13.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@meese-os/event-emitter": "workspace:*",
    "async-dependency-graph": "^1.1.4",
    "deepmerge": "^4.2.2",
    "omit-deep": "^0.3.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@meese-os/eslint-config"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "esdoc": {
    "source": "./src",
    "destination": "./doc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "brand": {
            "title": "meeseOS Common API",
            "description": "meeseOS Common API Documentation"
          },
          "lint": {
            "enable": false
          },
          "coverage": {
            "enable": false
          },
          "undocumentIdentifier": {
            "enable": false
          }
        }
      },
      {
        "name": "esdoc-publish-html-plugin"
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "all": true
        }
      },
      {
        "name": "./src/esdoc.js"
      }
    ]
  }
}
